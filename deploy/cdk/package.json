{
  "name": "torenken-doc-box",
  "repository": {
    "type": "git",
    "url": "git@github.com:torenken/doc-box.git"
  },
  "scripts": {
    "build": "npx projen build",
    "bundle": "npx projen bundle",
    "clobber": "npx projen clobber",
    "compile": "npx projen compile",
    "default": "npx projen default",
    "deploy": "npx projen deploy",
    "destroy": "npx projen destroy",
    "diff": "npx projen diff",
    "eject": "npx projen eject",
    "eslint": "npx projen eslint",
    "package": "npx projen package",
    "post-compile": "npx projen post-compile",
    "post-upgrade": "npx projen post-upgrade",
    "pre-compile": "npx projen pre-compile",
    "synth": "npx projen synth",
    "synth:silent": "npx projen synth:silent",
    "test": "npx projen test",
    "test:watch": "npx projen test:watch",
    "upgrade": "npx projen upgrade",
    "watch": "npx projen watch",
    "projen": "npx projen",
    "postinstall": "touch node_modules/go.mod",
    "audit:check": "yarn audit:level-high && yarn audit:level-critical",
    "audit:level-high": "/bin/bash -c 'yarn audit --groups dependencies; [[ $? -ge 8 ]] && exit 1 || exit 0'",
    "audit:level-critical": "/bin/bash -c 'yarn audit --groups devDependencies; [[ $? -ge 16 ]] && exit 1 || exit 0'",
    "snyk:iac": "cd cdk.out && snyk iac test --policy-path=../.snyk --severity-threshold=high --report"
  },
  "author": {
    "name": "Thomas Renken",
    "organization": false
  },
  "devDependencies": {
    "@types/jest": "28",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "aws-cdk": "^2.60.0",
    "esbuild": "^0.15.13",
    "eslint": "^8",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "jest": "28",
    "jest-junit": "^13",
    "json-schema": "^0.4.0",
    "npm-check-updates": "^16",
    "projen": "^0.67.0",
    "ts-jest": "28",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-go-alpha": "^2.60.0-alpha.0",
    "aws-cdk-lib": "^2.60.0",
    "constructs": "^10.0.5"
  },
  "license": "MIT",
  "version": "0.0.0",
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
